{
  "name": "setup-lolcode-action",
  "version": "1.0.17",
  "description": "This GitHub action installs LCI - LOLCODE interpreter",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:fabasoad/setup-lolcode-action.git"
  },
  "scripts": {
    "build": "ncc build -m src/index.ts",
    "lint": "eslint --ext ts src",
    "postinstall": "pre-commit install --hook-type pre-commit && pre-commit install --hook-type pre-push",
    "test": "jest --config=jest.config.json --coverage"
  },
  "keywords": [
    "actions",
    "programming-language"
  ],
  "author": "Yevhen Fabizhevskyi",
  "license": "MIT",
  "homepage": "https://github.com/fabasoad/setup-lolcode-action#readme",
  "dependencies": {
    "@actions/core": "1.9.1",
    "@actions/tool-cache": "2.0.1",
    "command-exists": "1.2.9",
    "glob": "8.0.3",
    "winston": "3.8.2"
  },
  "devDependencies": {
    "@types/chai": "4.3.3",
    "@types/command-exists": "1.2.0",
    "@types/glob": "8.0.0",
    "@types/jest": "29.0.3",
    "@types/node": "18.7.23",
    "@types/sinon": "10.0.13",
    "@typescript-eslint/eslint-plugin": "5.38.1",
    "@typescript-eslint/parser": "5.38.1",
    "@vercel/ncc": "0.34.0",
    "chai": "4.3.6",
    "eslint": "8.24.0",
    "eslint-config-google": "0.14.0",
    "jest": "29.0.3",
    "jest-circus": "29.0.3",
    "mocha-param": "2.0.1",
    "sinon": "14.0.0",
    "ts-jest": "29.0.2",
    "typescript": "4.8.3"
  }
}
